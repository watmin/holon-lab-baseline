# WordPress LEMP Stack Configuration
# Copy this to .env and fill in your values
# Required variables will cause KeyError if not set

# MySQL (required)
MYSQL_PASSWORD=your_mysql_password
MYSQL_ROOT_PASSWORD=your_mysql_root_password

# WordPress Admin (required for automation scripts)
WP_ADMIN_USER=admin
WP_ADMIN_PASSWORD=your_wp_admin_password

# WordPress URL (required)
WORDPRESS_URL=https://your-wordpress-host

# WordPress Application Password (required for REST API scripts)
WP_APP_PASSWORD=xxxx xxxx xxxx xxxx xxxx xxxx

# Ollama LLM (required for traffic generator)
OLLAMA_HOST=http://your-ollama-host:11434
OLLAMA_MODEL=qwen2.5:14b

# Network (adjust for your LAN)
NGINX_IP=192.168.1.200
LAN_SUBNET=192.168.1.0/24
LAN_GATEWAY=192.168.1.1
PARENT_INTERFACE=eno1
